<div style="background-image: url(<%= asset_path "background.jpg" %>)" class="background-landing">
  <% if user_signed_in? %>
    <%= link_to "groups", groups_path, class: "btn btn-danger" %>
    <%= link_to "events", event_users_path, class: "btn btn-danger" %>
    <%= link_to "invitations", usergroups_path(current_user), class: "btn btn-danger" %>
  <% end %>
</div>
<div class="frame">
	<div class="button-landing-sign">
		<span>Sign Up!</span>
		<svg>
		<polyline class="o1" points="0 0, 150 0, 150 55, 0 55, 0 0"></polyline>
		<polyline class="o2" points="0 0, 150 0, 150 55, 0 55, 0 0"></polyline>
	</svg>
	</div>

</div>
